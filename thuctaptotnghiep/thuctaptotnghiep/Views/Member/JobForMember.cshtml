@model IList<Model.Framework.DangTinViecLam>
@{
    ViewBag.Title = "JobForMember";
}
<div id="profileLevel" class="panel panel-default section-sidebar section-profile-level">

    <div class="panel-heading">
        Việc làm dành cho bạn
        <span id="loading-profile-level" class="pull-right" hidden=""><i class="fa fa-spinner fa-pulse"></i></span>
    </div>
    <div class="panel-content box-sm">
        <div>
            @foreach (var item in Model)
            {
                string namejob = new thuctaptotnghiep.Models.RemoveUnicode().UnicodeName(item.TenCV);
            <p>
                <a href="@Url.Action("DetailJob", "Job", new { TenCV=namejob,id = item.MACV })" title="@item.CongTy.Tencongty"><i style="margin-right:8px;" class="fa fa-hand-o-right" aria-hidden="true"></i>@item.TenCV</a>
            </p>
            }
        </div>
    </div>
</div>

